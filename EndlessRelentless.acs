return Deelay;
}




Script "Soil" (void)
{
int Direction, counter, i, tx;




while(TRUE)
	{
	counter = 0;
	while( counter < Soil[None][SoilParam_COUNT])
		{
		counter++;
					if (RoundInfo[SoilRestart])
						{

						Switch(Soil[counter][SoilParam_TYPE])
							{
							case SoilICE:	tx = FlatICEYtx;		break;
							case SoilMUD:	tx = FlatMUDDtx;		break;
