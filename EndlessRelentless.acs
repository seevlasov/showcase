				}

			HudMessageBold	(s:String; HUDMSG_FADEINOUT|HUDMSG_ALPHA, HUDid_AddBackground, t,	int2fix(i), 					int2fix(HUDY_VERYBIG/2 + HUDY_VERYBIG/4 - 5), MissionFailed_Hold, 		noteFadeIn_Standart, noteFadeOut_Standart, AdvertAlpha);

	}




