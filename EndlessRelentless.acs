function int GetShopSelection (int Pitch, int Max)
{
Pitch = Pitch + MaxClientPitch;
int counter, Num;
int Segment = (MaxClientPitch*2)/Max;


while(counter < Max)
	{

	if (Pitch <= 0) break;		//0


	if ((Pitch >= Segment*counter) && (Pitch < Segment*(counter + 1)))	break;
	counter++;
	}
if (counter >= Max) counter = Max - 1;
return counter;
}



