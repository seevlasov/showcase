												ChangeFloor	(TagAt (Soil[counter][SoilParam_BLOCKCOUNTER], ToRIGHT, i), TextureIs(tx));
								}
						delay(5);
						}


		}
	RoundInfo[SoilRestart] = FALSE;
	delay(1);
	}
}


